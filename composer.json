{
  "name": "alecrabbit/php-twrb-core",
  "type": "library",
  "description": "The White Rabbit Bot core library",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "alecrabbit/php-accessories": "^0.12.0",
    "alecrabbit/php-helpers": "^0.6.3",
    "krowinski/bcmath-extended": "^5.0",
    "react/event-loop": "^1.1",
    "reactivex/rxphp": "^2.0",
    "alecrabbit/php-console-spinner": "^0.14.7"
  },
  "require-dev": {
    "jaggedsoft/php-binance-api": "^0.5.6",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "rx/websocket": "^2.1",
    "symfony/var-dumper": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\TWRBCore\\": "src\\TWRBCore"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\TWRBCore\\": "tests"
    },
    "files": [
      "src/TWRBCore/Constants.php"
    ]
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
